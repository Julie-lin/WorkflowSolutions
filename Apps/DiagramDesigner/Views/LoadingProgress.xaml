<Window x:Class="DiagramDesigner.Views.LoadingProgress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadingProgress" Height="250" Width="300">
    <Grid Height="102" Margin="0,0,-0.4,0" VerticalAlignment="Top">
        <ProgressBar Height="50" Margin="0,20,0,0"  HorizontalAlignment="Stretch" IsIndeterminate="True" />
        <Label
        	x:Name="lblProgress"
        	Content="Loading."
        	Foreground="White"
        	FontWeight="Bold"
        	HorizontalAlignment="Left"
        	VerticalAlignment="Top"
        	Margin="0,138.8,0,-62.4" />
    </Grid>
</Window>
